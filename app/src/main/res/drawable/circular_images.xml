<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval"
            android:innerRadius="5dp">
            <stroke android:color="#284942"
                android:width="8dp"/>
            <padding android:left="10dp" android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval"
            android:innerRadius="5dp">
            <stroke android:color="#003300"
                android:width="3dp"/>
            <padding android:top="15dp" android:right="15dp"
                android:bottom="15dp" android:left="15dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:dashWidth="1dp"/>
            <padding android:top="10dp" android:right="10dp"
                android:bottom="10dp" android:left="10dp"/>
        </shape>
    </item>

</layer-list>